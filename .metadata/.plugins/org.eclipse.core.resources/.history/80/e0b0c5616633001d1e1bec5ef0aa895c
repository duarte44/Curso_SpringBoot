package com.allan.cursomc.services.exceptions;

public class DataIntefrityViolationException extends RuntimeException {

	
	private static final long serialVersionUID =1L;
	
	public DataIntefrityViolationException(String msg) {
		super(msg);
	}
	
	public DataIntefrityViolationException(String msg, Throwable cause) {
		super(msg, cause);
		/* recebe uma exceção que pode ter ocorrido antes */
	}
	
}
